{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      username: '',\n      password: ''\n    };\n  },\n  methods: {\n    async submitForm() {\n      if (this.validateForm()) {\n        await this.login();\n      }\n    },\n    validateForm() {\n      const usernameLength = this.username.length;\n      const passwordLength = this.password.length;\n      if (usernameLength < 3 || usernameLength > 20) {\n        alert('用户名必须在3到20个字符之间');\n        return false;\n      }\n      if (passwordLength < 6 || passwordLength > 20) {\n        alert('密码必须在6到20个字符之间');\n        return false;\n      }\n      return true;\n    },\n    async login() {\n      const response = await fetch('/api/login', {\n        method: 'POST',\n        headers: {\n          'Content-Type': 'application/json'\n        },\n        body: JSON.stringify({\n          username: this.username,\n          password: this.password\n        })\n      });\n      const data = await response.json();\n      if (data.code === 200) {\n        alert('登录成功');\n        // 处理登录成功后的逻辑，保存token\n        localStorage.setItem('token', data.data.token);\n        this.$router.push({\n          name: 'Start'\n        }); // 登录成功后跳转到 Start 页面\n      } else if (data.code === 504) {\n        alert('用户名或密码错误');\n      } else {\n        alert('登录失败');\n      }\n    },\n    register() {\n      this.$router.push('/register');\n    }\n  }\n};","map":{"version":3,"names":["data","username","password","methods","submitForm","validateForm","login","usernameLength","length","passwordLength","alert","response","fetch","method","headers","body","JSON","stringify","json","code","localStorage","setItem","token","$router","push","name","register"],"sources":["D:\\VUEitem\\front\\src\\components\\Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"login-container\">\r\n    <div class=\"login-box\">\r\n      <h2>登录</h2>\r\n      <form @submit.prevent=\"submitForm\">\r\n        <div class=\"input-group\">\r\n          <label for=\"username\">用户名</label>\r\n          <input type=\"text\" id=\"username\" v-model=\"username\" required minlength=\"3\" maxlength=\"20\">\r\n        </div>\r\n        <div class=\"input-group\">\r\n          <label for=\"password\">密码</label>\r\n          <input type=\"password\" id=\"password\" v-model=\"password\" required minlength=\"6\" maxlength=\"20\">\r\n        </div>\r\n        <div class=\"button-group\">\r\n          <button type=\"button\" @click=\"register()\">注册账号</button>\r\n          <button type=\"submit\">确认登录</button>\r\n        </div>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      username: '',\r\n      password: ''\r\n    };\r\n  },\r\n  methods: {\r\n    async submitForm() {\r\n      if (this.validateForm()) {\r\n        await this.login();\r\n      }\r\n    },\r\n    validateForm() {\r\n      const usernameLength = this.username.length;\r\n      const passwordLength = this.password.length;\r\n\r\n      if (usernameLength < 3 || usernameLength > 20) {\r\n        alert('用户名必须在3到20个字符之间');\r\n        return false;\r\n      }\r\n\r\n      if (passwordLength < 6 || passwordLength > 20) {\r\n        alert('密码必须在6到20个字符之间');\r\n        return false;\r\n      }\r\n\r\n      return true;\r\n    },\r\n    async login() {\r\n      const response = await fetch('/api/login', {\r\n        method: 'POST',\r\n        headers: {\r\n          'Content-Type': 'application/json'\r\n        },\r\n        body: JSON.stringify({\r\n          username: this.username,\r\n          password: this.password\r\n        })\r\n      });\r\n\r\n      const data = await response.json();\r\n      if (data.code === 200) {\r\n        alert('登录成功');\r\n        // 处理登录成功后的逻辑，保存token\r\n        localStorage.setItem('token', data.data.token);\r\n        this.$router.push({ name: 'Start' }); // 登录成功后跳转到 Start 页面\r\n      } else if (data.code === 504) {\r\n        alert('用户名或密码错误');\r\n      } else {\r\n        alert('登录失败');\r\n      }\r\n    },\r\n    register() {\r\n      this.$router.push('/register');\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.login-container {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  height: 100vh;\r\n  background-color: #f5f5f5;\r\n  font-family: \"宋体\", sans-serif;\r\n}\r\n\r\n.login-box {\r\n  width: 320px;\r\n  padding: 30px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 15px;\r\n  background-color: #fff;\r\n  text-align: center;\r\n}\r\n\r\nh2 {\r\n  margin-bottom: 20px;\r\n  font-size: 24px;\r\n}\r\n\r\n.input-group {\r\n  margin-bottom: 20px;\r\n  text-align: left;\r\n}\r\n\r\nlabel {\r\n  display: block;\r\n  margin-bottom: 10px;\r\n  font-size: 18px;\r\n}\r\n\r\ninput {\r\n  width: calc(100% - 25px);\r\n  padding: 10px;\r\n  font-size: 18px;\r\n  border: none;\r\n  border-radius: 15px;\r\n  background-color: #eee;\r\n}\r\n\r\n.button-group {\r\n  display: flex;\r\n  justify-content: space-around;\r\n  margin-top: 20px;\r\n}\r\n\r\nbutton {\r\n  padding: 10px 20px;\r\n  font-size: 18px;\r\n  color: #fff;\r\n  background-color: #555;\r\n  border: none;\r\n  border-radius: 15px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease, opacity 0.3s ease;\r\n  font-family: \"宋体\", sans-serif;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #64ffc1; /* 浅绿色 */\r\n  opacity: 0.8;\r\n}\r\n</style>\r\n"],"mappings":";AAuBA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,UAAUA,CAAA,EAAG;MACjB,IAAI,IAAI,CAACC,YAAY,CAAC,CAAC,EAAE;QACvB,MAAM,IAAI,CAACC,KAAK,CAAC,CAAC;MACpB;IACF,CAAC;IACDD,YAAYA,CAAA,EAAG;MACb,MAAME,cAAa,GAAI,IAAI,CAACN,QAAQ,CAACO,MAAM;MAC3C,MAAMC,cAAa,GAAI,IAAI,CAACP,QAAQ,CAACM,MAAM;MAE3C,IAAID,cAAa,GAAI,KAAKA,cAAa,GAAI,EAAE,EAAE;QAC7CG,KAAK,CAAC,iBAAiB,CAAC;QACxB,OAAO,KAAK;MACd;MAEA,IAAID,cAAa,GAAI,KAAKA,cAAa,GAAI,EAAE,EAAE;QAC7CC,