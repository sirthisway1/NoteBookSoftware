nity-button {\r\n      background-color: #696969;\r\n      color: white;\r\n      border-radius: 10px;\r\n      padding: 10px 20px;\r\n    }\r\n  \r\n    .community {\r\n      flex: 1;\r\n      padding: 20px;\r\n      overflow-y: auto;\r\n    }\r\n  \r\n    .note {\r\n      display: flex;\r\n      flex-direction: column;\r\n      margin-bottom: 20px;\r\n      padding: 10px;\r\n      border: 1px solid #ccc;\r\n      border-radius: 8px;\r\n    }\r\n  \r\n    .note-user {\r\n      font-weight: bold;\r\n    }\r\n  \r\n    .note-title {\r\n      font-size: 20px;\r\n      font-weight: bold;\r\n      margin: 10px 0;\r\n    }\r\n  \r\n    .note-content {\r\n      font-size: 16px;\r\n    }\r\n  </style>"],"mappings":";;;EACSA,KAAK,EAAC;AAAW;;EACfA,KAAK,EAAC;AAAS;;EACbA,KAAK,EAAC,cAAc;EAACC,EAAE,EAAC;;gEAI3BC,mBAAA,CAAgB,cAAV,KAAG;oBAATC,UAAgB,C;gEAGdD,mBAAA,CAAoC;EAA/BF,KAAK,EAAC;AAAkB;gEAC7BE,mBAAA,CAAiB,cAAX,MAAI;oBADVE,UAAoC,EACpCC,UAAiB,C;;EAKlBL,KAAK,EAAC;AAAc;;EAClBA,KAAK,EAAC;AAAW;iEACpBE,mBAAA,CAAa,YAAT,MAAI;;EAEDF,KAAK,EAAC;AAAW;;EACjBA,KAAK,EAAC;AAAY;;EAClBA,KAAK,EAAC;AAAc;;uBArBjCM,mBAAA,CAyBM,OAzBNC,UAyBM,GAxBJL,mBAAA,CAaM,OAbNM,UAaM,GAZJN,mBAAA,CAA4D,OAA5DO,UAA4D,EAAAC,gBAAA,CAAjBC,KAAA,CAAAC,QAAQ,kBACnDV,mBAAA,CAAqD;IAAhDF,KAAK,EAAC,cAAc;IAAEa,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEC,QAAA,CAAAC,SAAA,IAAAD,QAAA,CAAAC,SAAA,IAAAF,IAAA,CAAS;KAAE,IAAE,GAC/Cb,mBAAA,CAGM;IAHDF,KAAK,EAAC,cAAc;IAAEa,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEC,QAAA,CAAAE,YAAA,IAAAF,QAAA,CAAAE,YAAA,IAAAH,IAAA,CAAY;KALtD,IAAAI,UAAA,IASQjB,mBAAA,CAIM;IAJDF,KAAK,EAAC,+BAAgC;IAAEa,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEC,QAAA,CAAAI,aAAA,IAAAJ,QAAA,CAAAI,aAAA,IAAAL,IAAA,CAAa;KATzE,IAAAM,UAAA,IAcQnB,mBAAA,CAAsD;IAAjDF,KAAK,EAAC,cAAc;IAAEa,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEC,QAAA,CAAAM,QAAA,IAAAN,QAAA,CAAAM,QAAA,IAAAP,IAAA,CAAQ;KAAE,MAAI,E,GAElDb,mBAAA,CASM,OATNqB,UASM,GARJrB,mBAAA,CAOM,OAPNsB,WAOM,GANJC,WAAa,G,kBACbnB,mBAAA,CAIMoB,SAAA,QAvBhBC,WAAA,CAmB2CX,QAAA,CAAAY,aAAa,EAArBC,I